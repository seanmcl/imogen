  =>

% axiom2, axiom.
(( exists X1. ( exists Y1. ( s(Y1, X1) | g(X1) ) ) ))

  =>

% axiom3, axiom.
(( forall X2. ( g(X2) <=> ( ( exists Y2. s(X2, Y2) ) | ( exists Z. ( r(Z) | ( q(Z) | s(a, Z) ) ) ) ) ) ))

  =>

% con, conjecture.
(( ( exists X3. q(X3) ) | ( ( exists X4. r(X4) ) | ( exists X5. ( exists Y5. s(X5, Y5) ) ) ) ))

 

