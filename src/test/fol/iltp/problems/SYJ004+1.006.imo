% ax1, axiom.
(( forall X. append(nil, X, X) ))

  =>

% ax2, axiom.
(( forall X Y Z W. ( append(Y, Z, W) => append(cons(X, Y), Z, cons(X, W)) ) ))

  =>

% con, conjecture.
(( exists X. append(cons(a1, cons(a2, cons(a3, cons(a4, cons(a5, cons(a6, nil)))))), nil, X) ))

 

