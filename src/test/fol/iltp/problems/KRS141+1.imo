% axiom_0, axiom.
(( forall X. ( cowlThing(X) & ( ~ cowlNothing(X) ) ) ))

  =>

% axiom_1, axiom.
(( forall X. ( xsd_string(X) <=> ( ~ xsd_integer(X) ) ) ))

  =>

% axiom_2, axiom.
(( forall X. ( cr(X) => ( forall Y. ( rp(X, Y) => cc(Y) ) ) ) ))

  =>

% axiom_3, axiom.
(cr(ii))

  =>

% axiom_4, axiom.
(cowlThing(ii))

  =>

% axiom_5, axiom.
(rp(ii, io))

  =>

% axiom_6, axiom.
(cowlThing(io))

  =>

% the_axiom, conjecture.
(( ( forall X. ( cowlThing(X) & ( ~ cowlNothing(X) ) ) ) & ( ( forall X. ( xsd_string(X) <=> ( ~ xsd_integer(X) ) ) ) & ( cc(io) & cowlThing(io) ) ) ))

 

