-*- mode: unicode -*-

--------------------------------------------------------------------------------
 Simp1
--------------------------------------------------------------------------------

   ⊤ ∧ ψ ↪ Ψ
   ψ ∧ ⊤ ↪ Ψ
   ψ ⊃ ⊤ ↪ ⊤
   ⊤ ⊃ ψ ↪ Ψ
   ∃ w. w = w' ∧ Ψ ↪ Ψ{w ↦ w'}
   ∃ w. w' = w ∧ Ψ ↪ Ψ{w ↦ w'}
   ∃ w. Ψ ∧ w = w' ↪ Ψ{w ↦ w'}
   ∃ w. Ψ ∧ w' = w ↪ Ψ{w ↦ w'}
   ∀ w. Ψ1 ∧ Ψ2 ↪ (∀ w. Ψ1) ∧ Ψ2   w ∉ Ψ2
   ∀ w. Ψ1 ∧ Ψ2 ↪ Ψ1 ∧ (∀ w. Ψ2)   w ∉ Ψ1
   ∃ w. Ψ1 ∧ Ψ2 ↪ (∃ w. Ψ1) ∧ Ψ2   w ∉ Ψ2
   ∃ w. Ψ1 ∧ Ψ2 ↪ Ψ1 ∧ (∃ w. Ψ2)   w ∉ Ψ1

--------------------------------------------------------------------------------
 Simp
--------------------------------------------------------------------------------

  Ψ1 ↪ Ψ1'       Ψ2 ↪ Ψ2'
--------------------------------
      Ψ1 ∧ Ψ2 ↪ Ψ1' ∧ Ψ2'


  Ψ1 ↪ Ψ1'       Ψ2 ↪ Ψ2'
--------------------------------
      Ψ1 ⊃ Ψ2 ↪ Ψ1' ⊃ Ψ2'


       Ψ ↪ Ψ'
----------------------
  ∀ w. Ψ ↪ ∀ w. Ψ'


       Ψ ↪ Ψ'
------------------------
  ∃ w. Ψ ↪ ∃ w. Ψ'
