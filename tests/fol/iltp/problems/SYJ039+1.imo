% axiom1, axiom.
(( forall X. ( ( ~ frUit(X) ) => ( ( ~ apple(X) ) & ( ~ pear(X) ) ) ) ))

  =>

% axiom2, axiom.
(( forall X. ( ( frUit(X) | ( bread(X) | ( cheese(X) | whiskeY(X) ) ) ) => food(X) ) ))

  =>

% axiom3, axiom.
(pear(moltke))

  =>

% axiom4, axiom.
(apple(grannYsmith))

  =>

% axiom5, axiom.
(apple(reddelicioUs))

  =>

% axiom6, axiom.
(cheese(stilton))

  =>

% axiom7, axiom.
(( apple(jUicYfrUit) | pear(jUicYfrUit) ))

  =>

% axiom8, axiom.
(( bread(rYe) | whiskeY(rYe) ))

  =>

% axiom9, axiom.
(( ( ~ cheese(brie) ) => frUit(brie) ))

  =>

% con, conjecture.
(( ~ ( ~ ( exists X. food(X) ) ) ))

 

